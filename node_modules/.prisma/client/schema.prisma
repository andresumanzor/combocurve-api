generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["fullTextSearch"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Movie {
    id                       Int                         @id @default(autoincrement())
    name                     String
    ratings                  Int
    durationInMinutes        Int
    createdAt                DateTime                    @default(now()) @db.Timestamptz(6)

    @@map("Movies")
}

